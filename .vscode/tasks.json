{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "cds-deploy-to-hana-MyHANAApp-jerry",
      "type": "shell",
      "command": "cds",
      "args": [
        "deploy",
        "--to",
        "hana:jerry",
        "--no-save"
      ],
      "options": {
        "cwd": "${workspaceFolder}/MyHANAApp"
      },
      "isBackground": true,
      "problemMatcher": [
        "$cap-hdi-bind-handler"
      ]
    },
