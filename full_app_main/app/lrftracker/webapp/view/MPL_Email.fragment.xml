<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog 
    
        confirmButtonText="Assign"
        noDataText="No Records Found"
        title="Select Email ID for LRF-{modelAssignEmail>/Lrf_No}"
        liveChange="_handleValueHelpSearchMPL"
        search="_handleValueHelpSearchMPL"
        confirm="_handleValueHelpConfirmMPL"
        cancel="_handleValueHelpClose"
        items="{
												path: 'EmailModel>/Users/'
											}"
    >
        <StandardListItem
            title="{EmailModel>Email}"
            type="Active"
        />
        
    </SelectDialog>
</core:FragmentDefinition>

