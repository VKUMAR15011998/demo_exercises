<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        noDataText="No Records Found"
        title="Select PO Number"
        liveChange="_handleValueHelpSearchPO"
        search="_handleValueHelpSearchPO"
        confirm="_handleValueHelpConfirmPO"
        cancel="_handleValueHelpClose"
        items="{
												path: 'modelTrk>/'
											}"
    >
        <StandardListItem
            title="{modelTrk>PO_Number}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>
